{
    "diagnostic.virtualText": false,
    "python.jediEnabled": false,
    "java.import.maven.enabled": false,
    "java.errors.incompleteClasspath.severity": "ignore",
    "languageserver": {
        "clojure-lsp": {
            "command": "bash",
            "args": [
                "-c",
                "clojure-lsp"
            ],
            "filetypes": [
                "clojure"
            ],
            "rootPatterns": [
                "project.clj",
                "build.boot",
                "deps.edn"
            ],
            "additionalSchemes": [
                "jar",
                "zipfile"
            ],
            "trace.server": "verbose",
            "initializationOptions": {}
        },
        "terraform": {
            "command": "terraform-lsp",
            "filetypes": [
                "terraform",
                "tf"
            ],
            "initializationOptions": {}
        }
    }
}
