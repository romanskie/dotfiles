## alias stuff
alias dot='$HOME/dotfiles'
alias dev='$HOME/dev'
alias ping='ping -c 5'

## a quick way to get out of current directory ##
alias ..='cd ..'
alias ...='cd ../../../'
alias ....='cd ../../../../'
alias c='clear'

# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
